<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>RightScale.netClient.ActivityLibrary</id>
    <version>0.2.1-alpha</version>
    <title>RightScale Activity Library</title>
